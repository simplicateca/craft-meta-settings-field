{
    "name": "simplicateca/metasettings",
    "description": "Meta Settings is a custom field plugin for Craft CMS 5+",
    "type": "craft-plugin",
    "keywords": ["craft-cms", "custom-field", "select", "select-field", "dropdown"],
    "license": "MIT",
    "authors": [{
        "name": "Simplicate",
        "homepage": "https://simplicate.ca"
    }],
    "support": {
        "email" : "info@simplicate.ca",
        "issues": "https://github.com/simplicateca/craft-meta-settings-field/issues?state=open",
        "source": "https://github.com/simplicateca/craft-meta-settings-field",
        "docs"  : "https://github.com/simplicateca/craft-meta-settings-field/blob/main/README.md"
    },
    "require": {
        "craftcms/cms": "^5.0.0"
    },
    "autoload": {
        "psr-4": {
            "simplicateca\\metasettings\\": "src/"
        }
    },
    "extra": {
        "name": "MetaSettings",
        "handle": "metasettings",
        "class": "simplicateca\\metasettings\\MetaSettings"
    }
}